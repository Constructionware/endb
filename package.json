{
    "name": "endb",
    "version": "0.16.3",
    "description": "Simple key-value storage for multi adapter.",
    "main": "src/index.js",
    "scripts": {
        "test": "ava && npm run lint",
        "test:watch": "ava --watch",
        "lint": "xo src",
        "build:docs": "jsdoc -c .jsdoc.json",
        "prepublishOnly": "npm run test"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/chroventer/endb.git"
    },
    "keywords": [
        "database",
        "store",
        "key-value",
        "adapter",
        "cache"
    ],
    "author": "chroventer <chroventer@gmail.com> (https://github.com/chroventer)",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/chroventer/endb/issues"
    },
    "homepage": "https://endb.js.org",
    "devDependencies": {
        "ava": "^2.4.0",
        "jsdoc": "^3.6.3",
        "xo": "^0.25.3"
    },
    "xo": {
        "prettier": true,
        "rules": {
            "guard-for-in": 0
        }
    }
}
