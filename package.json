{
    "name": "endb",
    "version": "0.19.2",
    "description": "Simple key-value storage with support for multiple backends",
    "main": "src/index.js",
    "typings": "typings/index.d.ts",
    "scripts": {
        "test": "ava --serial",
        "test:watch": "ava --watch",
        "lint": "xo src",
        "lint:fix": "xo src --fix",
        "build:docs": "jsdoc -c .jsdoc.json",
        "prepublishOnly": "npm run test"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/chroventer/endb.git"
    },
    "keywords": [
        "endb",
        "key-value",
        "cache",
        "javascript"
    ],
    "author": "chroventer <chroventer@gmail.com> (https://github.com/chroventer)",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/chroventer/endb/issues"
    },
    "homepage": "https://endb.js.org",
    "devDependencies": {
        "ava": "^2.4.0",
        "better-docs": "^1.4.7",
        "jsdoc": "^3.6.3",
        "xo": "^0.25.3"
    },
    "xo": {
        "prettier": true,
        "semicolon": true,
        "rules": {
            "guard-for-in": 0,
            "no-unused-vars": 0
        }
    }
}
