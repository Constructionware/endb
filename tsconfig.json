{
  "compilerOptions": {
    "target": "ES6",
    "module": "commonjs",
    "allowJs": true,
    "declaration": true,
    "outDir": "./dist",
    "rootDir": "./src",
    "strict": true,
    "moduleResolution": "node",
    "esModuleInterop": true
  },
  "typedocOptions": {
    "name": "Endb",
    "readme": "README.md",
    "out": "docs",
    "hideGenerator": true
  },
  "include": ["src", "test"]
}
