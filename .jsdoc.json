{
  "plugins": ["plugins/markdown"],
  "source": {
    "include": ["src", "README.md", "package.json"]
  },
  "sourceType": "module",
  "opts": {
    "template": "node_modules/minami",
    "encoding": "utf8",
    "destination": "docs/",
    "readme": "README.md",
    "recurse": true,
    "verbose": true
  },
  "templates": {
    "collapse": true,
    "cleverLinks": false,
    "monospaceLinks": false,
    "linenums": true
  }
}
