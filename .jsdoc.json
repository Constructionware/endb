{
  "plugins": ["plugins/markdown"],
  "source": {
    "include": ["src", "README.md"]
  },
  "sourceType": "module",
  "opts": {
    "template": "node_modules/better-docs",
    "encoding": "utf8",
    "destination": "docs",
    "readme": "README.md",
    "recurse": true,
    "verbose": true
  },
  "templates": {
    "collapse": true,
    "cleverLinks": false,
    "monospaceLinks": false,
    "linenums": true,
    "better-docs": {
      "name": "Endb",
      "logo": "media/logo.png",
      "hideGenerator": true,
      "navigation": [
        {
          "label": "GitHub",
          "href": "https://github.com/chroventer/endb"
        },
        {
          "label": "NPM",
          "href": "https://npmjs.com/package/endb"
        }
      ]
    }
  }
}
