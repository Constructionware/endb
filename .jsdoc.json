{
    "plugins": [
        "plugins/markdown",
        "plugins/summarize"
    ],
    "source": {
        "include": [
            "src",
            "README.md"
        ],
        "includePattern": ".js$",
        "excludePattern": "(node_modules/|docs/|test/)"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": [
            "jsdoc"
        ]
    },
    "opts": {
        "template": "./node_modules/better-docs",
        "encoding": "utf8",
        "destination": "docs/",
        "readme": "README.md",
        "recurse": true,
        "verbose": true
    },
    "templates": {
        "collapse": true,
        "cleverLinks": false,
        "monospaceLinks": false,
        "linenums": true,
        "better-docs": {
            "name": "Endb",
            "logo": "./media/logo.png",
            "trackingCode": "UA-149839118-1",
            "navigation": [
                {
                    "label": "GitHub",
                    "href": "https://github.com/endb/endb"
                },
                {
                    "label": "NPM",
                    "href": "https://npmjs.com/package/endb"
                }
            ]
        }
    }
}